{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "principalType": {
      "value": "${AZURE_PRINCIPAL_TYPE}"
    },
    "useVirtualNetworkIntegration": {
      "value": "${USE_VIRTUAL_NETWORK_INTEGRATION}"
    },
    "useVirtualNetworkPrivateEndpoint": {
      "value": "${USE_VIRTUAL_NETWORK_PRIVATE_ENDPOINT}"
    },
    "virtualNetworkAddressSpacePrefix": {
      "value": "${VIRTUAL_NETWORK_ADDRESS_SPACE_PREFIX}"
    },
    "virtualNetworkIntegrationSubnetAddressSpacePrefix": {
      "value": "${VIRTUAL_NETWORK_INTEGRATION_SUBNET_ADDRESS_SPACE_PREFIX}"
    },
    "virtualNetworkPrivateEndpointSubnetAddressSpacePrefix": {
      "value": "${VIRTUAL_NETWORK_PRIVATE_ENDPOINT_SUBNET_ADDRESS_SPACE_PREFIX}"
    }
  }
}